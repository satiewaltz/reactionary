{
  "name": "reactionary-api",
  "description": "Express API for React-Redux-Links",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "reset": "rimraf ./functions/build && cd ./functions/ && yarn install",
    "prepackage-functions": "rimraf functions",
    "package-functions": "webpack --progress --display-error-details",
    "postpackage-functions": "cd functions && yarn && cd ..",
    "deploy": "yarn run package-functions && firebase deploy",
    "start": "node --experimental-modules ./src/index.mjs"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "copy-webpack-plugin": "^4.2.0",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2",
    "webpack": "^3.8.1",
    "webpack-node-externals": "^1.6.0"
  },
  "private": true
}
